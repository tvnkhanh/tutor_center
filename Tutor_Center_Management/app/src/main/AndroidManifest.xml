<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tutor_Center_Management"
        tools:targetApi="31">
        <activity
            android:name=".view.auth.DetailScreenActivity"
            android:exported="false" />
        <activity
            android:name=".view.auth.SignUpScreenActivity"
            android:exported="false" />
        <activity
            android:name=".view.auth.AuthScreenActivity"
            android:exported="false" />
        <activity
            android:name=".view.auth.SignInScreenActivity"
            android:exported="false" />
        <activity
            android:name=".ui.login.LoginScreenActivity"
            android:exported="false"
            android:label="@string/title_activity_login_screen" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".StartUpScreenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>